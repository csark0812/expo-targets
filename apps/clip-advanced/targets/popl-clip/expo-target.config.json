{
  "type": "clip",
  "name": "PoplClip",
  "displayName": "Popl Contact",
  "platforms": ["ios"],
  "appGroup": "group.com.test.clipadvanced",
  "ios": {
    "colors": {
      "AccentColor": { "light": "#000000", "dark": "#FFFFFF" },
      "BackgroundColor": { "light": "#F9F4EF", "dark": "#1C1C1E" },
      "PrimaryText": { "light": "#000000", "dark": "#FFFFFF" }
    },
    "entitlements": {
      "com.apple.developer.associated-domains": ["appclips:pro.poplme.co"]
    },
    "infoPlist": {
      "CFBundleURLTypes": [
        {
          "CFBundleURLSchemes": ["com.nfc.popl"]
        }
      ],
      "NSAppTransportSecurity": {
        "NSExceptionDomains": {
          "localhost": {
            "NSExceptionAllowsInsecureHTTPLoads": true,
            "NSIncludesSubdomains": true
          }
        },
        "NSAllowsLocalNetworking": true
      }
    }
  }
}
