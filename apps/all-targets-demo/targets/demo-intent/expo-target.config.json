{
  "type": "intent",
  "name": "DemoIntent",
  "displayName": "Demo Intent",
  "platforms": ["ios"],
  "ios": {
    "infoPlist": {
      "NSExtension": {
        "NSExtensionPointIdentifier": "com.apple.intents-service",
        "NSExtensionPrincipalClass": "IntentHandler",
        "NSExtensionAttributes": {
          "IntentsSupported": [
            "INStartWorkoutIntent",
            "INPauseWorkoutIntent",
            "INEndWorkoutIntent",
            "INResumeWorkoutIntent"
          ]
        }
      }
    }
  }
}
